#include <iostream>

using namespace std;

int main(){

  int m,n;
  cin >> n >> m;
  int X[5][5] = {0}
  for(int i = 0; i < m; i++){
    for(int j = 0; j < n-2; j++){
      if (X[i][j] == X[i][j+1] && X[i][j] == X[i][j+2] && X[i][j] == 0){
        cout << "Yes" << endl;
        return 0;
      }
    }
  }
  cout << "No" << endl;
  return 0;
}
      
      
      
      
        
