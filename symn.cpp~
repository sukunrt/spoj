#include <vector>
#include <iostream>

using namespace std;
int main(){
  int n;
  vector <int> symmet(n);
  for(int i = 0; i < n; i++)
    cin >> symmet[i];
  int j = n-1;
  for(int i = 0; i < j; i++, j--){
    if (symmet[i] != symmet[j]){
      cout << "No" <<endl;
      return 0;
    }
  }
  cout << "Yes" << endl;
}

      
