#include <iostream>
#include <vector>

int tree[100005][1];
int main(){

  int n;
  scanf("%d", &n);
  make_tree(n);

  update(1, n, i+1, j+1);


}

void update(int start, int begin, int i, int j,int node){
  if (start > j || end < i || start > end)
    return;

  if (start >= i && end <= j){
    tree[node][0] = !tree[node][0];
    return;
  }

  int mid = (start + end)/2;
  update(start, mid, i, j, 2*node);
  update(mid+1, end, i, j , 2*node + 1);

  return;
}
   
  

  
    
    
  
  
  
